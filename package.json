{
  "name": "rails-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format-check": "prettier --check './**/*.{css,scss,json,md,js,ts,tsx,jsx}'",
    "format-fix": "prettier --write './**/*.{css,scss,json,md,js,ts,tsx,jsx}'",
    "js-lint": "eslint . --ignore-pattern '!.eslintrc.js' --ext js,ts,tsx,jsx",
    "js-lint-fix": "eslint . --ignore-pattern '!.eslintrc.js' --ext js,ts,tsx,jsx --fix",
    "scss-lint": "stylelint '**/*.{css,scss}'",
    "scss-lint-fix": "stylelint '**/*.{css,scss}' --fix"
  },
  "babel": {
    "presets": [
      "./node_modules/shakapacker/package/babel/preset.js"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "prettier": "prettier-config-ackama",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@hotwired/stimulus": "^3.1.0",
    "@hotwired/stimulus-webpack-helpers": "^1.0.1",
    "@sentry/browser": "^7.14.2",
    "babel-loader": "^8.0.0",
    "compression-webpack-plugin": "^9.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.1",
    "dotenv-webpack": "^8.0.1",
    "mini-css-extract-plugin": "^2.6.1",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "shakapacker": "^6.5.2",
    "terser-webpack-plugin": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-assets-manifest": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^8.24.0",
    "eslint-config-ackama": "^3.0.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lighthouse": "^9.6.7",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "prettier-config-ackama": "^1.1.0",
    "prettier-plugin-packagejson": "^2.3.0",
    "stylelint": "^14.13.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-scss": "^4.3.0"
  },
  "engines": {
    "node": "^16.15.0"
  }
}
